      "source": [
        "sentences = [\n",
        "                'I work at The University of British Columbia.',\n",
        "                'Vancouver is a beautiful city.',\n",
        "                'Apple is about to unveil the newest device.'\n",
        "            ]\n",
        "\n",
        "results = [nlp(sentence) for sentence in sentences]\n",
        "\n",
        "for res in results:\n",
        "    for ent in res.ents:\n",
        "        print(ent.text, ' -> ', ent.label_)"
      ],
